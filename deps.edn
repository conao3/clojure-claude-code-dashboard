{:paths ["src" "resources"]

 :deps
 {org.clojure/clojure {:mvn/version "1.12.3"}}

 :aliases
 {:dev {:extra-paths ["dev" "test"]
        :extra-deps {com.stuartsierra/component.repl {:mvn/version "1.0.0"}}}
  :cljs {:extra-deps {org.clojure/clojurescript {:mvn/version "1.12.116"}
                      thheller/shadow-cljs {:mvn/version "3.3.2"}
                      reagent/reagent {:mvn/version "2.0.1"}
                      no.cjohansen/portfolio {:mvn/version "2024.09.25"}
                      prismatic/schema {:mvn/version "1.4.1"}}}
  :repl {:extra-deps {nrepl/nrepl {:mvn/version "1.5.1"}
                      cider/cider-nrepl {:mvn/version "0.58.0"}}
         :jvm-opts ["-Djdk.attach.allowAttachSelf"]
         :main-opts ["-m" "nrepl.cmdline"
                     "--interactive"
                     "--middleware" "[cider.nrepl/cider-middleware]"]}}}
